{
    "name": "AnimaLib-test",
    "tree": {
        "$className": "DataModel",

        "Workspace": {
            "$className": "Workspace",
        
            "Baseplate": {
                "$className": "Part",
                "$properties": {
                    "Anchored": true,
                    "Locked": true,
                    "Size": {
                        "Vector3": [2048, 2, 2048]
                    },
                    "Color" : {
                        "Color3": [0.3, 0.3, 0.3]
                    }
                }
            }
        },

        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "Packages": {
                "$path": "Packages/",
                "AnimaLib": {
                    "$path": "src/"
                }
            }
        },

        "TestService": {
			"$className": "TestService",
            "$path": "tests/",
			"$properties": {
				"ExecuteWithStudioRun": true,
                "NumberOfPlayers": 0,
                "SimulateSecondsLag": 0,
                "Timeout": 10
			}
		}
    }
}